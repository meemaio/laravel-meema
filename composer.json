{
    "name": "meema/laravel-meema",
    "description": "This package a Laravel wrapper around the Meema API.",
    "keywords": [
        "meema",
        "cion",
        "laravel",
        "laravel-meema"
    ],
    "homepage": "https://github.com/meemaio/laravel-meema",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Breuer",
            "email": "chris@cion.agency",
            "homepage": "https://cion.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Meema\\LaravelMeema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meema\\LaravelMeema\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meema\\LaravelMeema\\MeemaServiceProvider"
            ],
            "aliases": {
                "Meema": "Meema\\LaravelMeema\\MeemaFacade"
            }
        }
    }
}
