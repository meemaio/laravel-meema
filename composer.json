{
    "name": "meema/laravel-meema",
    "description": "üêè A blazing-fast Laravel adapter for the official Meema API. An API each Laravel developer will feel welcome to.",
    "keywords": [
        "meema",
        "laravel",
        "laravel-meema",
        "media management",
        "image editing",
        "video converter"
    ],
    "homepage": "https://github.com/meemalabs/laravel-meema",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Breuer",
            "email": "chris@cion.agency",
            "homepage": "https://cion.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5 || ^8.0",
        "meema/meema-client-php": "@dev"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "pestphp/pest": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.0",
        "phpunit/phpunit": "^8.0|^9.3",
        "vlucas/phpdotenv": "^4.2|^5.3"
    },
    "autoload": {
        "psr-4": {
            "Meema\\LaravelMeema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meema\\LaravelMeema\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meema\\LaravelMeema\\Providers\\MeemaServiceProvider"
            ],
            "aliases": {
                "Meema": "Meema\\LaravelMeema\\Facades\\MeemaFacade"
            }
        }
    },
    "repositories": {
        "local": {
            "type": "path",
            "url": "../meema-php-sdk"
        }
    }
}
